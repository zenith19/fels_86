<h1 class="center"><%= t :users_title %></h1>

<%= will_paginate %>

<ul class="users">  
  <% @users.each do |user| %>
  <li>
    <h3><%= link_to user.firstname + " " + user.lastname, admin_user_path(user.id) %></h3>
    <%= link_to t(:update), edit_admin_user_path(user.id) %> 
  </li>
  <% end %>
</ul>

<%= will_paginate %>