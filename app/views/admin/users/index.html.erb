<h1><%= t :users_title %></h1>

<%= will_paginate %>

<ul class="users">  
  <% @users.each do |user| %>
  <li>
    <%= link_to user.lastname + " " + user.firstname, admin_user_path(user.id) %>
    <%= link_to t(:update), edit_admin_user_path(user.id) %> 
  </li>
  <% end %>
</ul>

<%= will_paginate %>